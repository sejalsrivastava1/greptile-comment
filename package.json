{
  "name": "greptile-comment",
  "displayName": "greptile-comment",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "type": "commonjs",
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "greptile-comment.addFunctionComment",
        "title": "Add Function Description"
      },
      {
        "command": "greptile-comment.indexRepository",
        "title": "Index Repository"
      },
      {
        "command": "greptile-comment.addComment",
        "title": "Add Greptile Comment"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "greptile-comment.indexRepository",
          "group": "4_search",
          "when": "editorFocus"
        },
        {
          "submenu": "greptile-comment",
          "group": "1_modification"
        }
      ],
      "greptile-comment": [
        {
          "command": "greptile-comment.addComment",
          "group": "1_modification@1"
        },
        {
          "command": "greptile-comment.addFunctionComment",
          "group": "1_modification@2"
        }
      ]
    },
    "submenus": [
      {
        "id": "greptile-comment",
        "label": "Add Comment"
      }
    ]
  },
  "scripts": {
    "compile": "npm run check-types && node esbuild.js",
    "check-types": "tsc --noEmit",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "vscode:prepublish": "npm run package",
    "package": "npm run check-types && node esbuild.js --production"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.92.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "esbuild": "^0.23.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@octokit/core": "^6.1.2",
    "axios": "^1.7.4",
    "uuid": "^10.0.0"
  },
  "extensionDependencies": [
    "vscode.git"
  ]
}
